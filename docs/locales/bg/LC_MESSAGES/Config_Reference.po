#~ "#driver_SEMIN: 0\n"
#~ "#driver_SEUP: 0\n"
#~ "#driver_SEMAX: 0\n"
#~ "#driver_SEDN: 0\n"
#~ "#driver_SEIMIN: 0\n"
#~ "#driver_SFILT: 0\n"
#~ "#   Set the given register during the configuration of the TMC5160\n"
