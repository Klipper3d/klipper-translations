"#pause_on_runout:\n"
"#runout_gcode:\n"
"#insert_gcode:\n"
"#event_delay:\n"
"#pause_delay:\n"
"#   See the \"filament_switch_sensor\" section for a description of the\n"
"#   above parameters.\n"
msgstr ""
"[hall_filament_width_sensor]\n"
"adc1:\n"
"adc2:\n"
