#~ "#driver_HSTRT: 5\n"
#~ "#driver_FD3: 0\n"
#~ "#driver_TPFD: 4\n"
#~ "#driver_CHM: 0\n"
#~ "#driver_VHIGHFS: 0\n"
#~ "#driver_VHIGHCHM: 0\n"
#~ "#driver_DISS2G: 0\n"
#~ "#driver_DISS2VS: 0\n"
#~ "#driver_PWM_AUTOSCALE: True\n"
#~ "#driver_PWM_AUTOGRAD: True\n"
#~ "#driver_PWM_FREQ: 0\n"
#~ "#driver_FREEWHEEL: 0\n"
#~ "#driver_PWM_GRAD: 0\n"
#~ "#driver_PWM_OFS: 30\n"
